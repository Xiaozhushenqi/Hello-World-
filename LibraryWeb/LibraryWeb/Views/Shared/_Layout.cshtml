<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>@ViewBag.Title</title>
    <link href="~/Content/Themes/Cop/css/Belvoly.min.css" rel="stylesheet" />
    <link href="~/Content/Themes/Base/css/font-awesome.css" rel="stylesheet" />
    <link href="~/Content/Themes/Base/jqueryui/1.11/jquery-ui.css" rel="stylesheet" />
    <link href="~/Content/Themes/Meeting/css/meeting.css" rel="stylesheet" />
    <link href="~/Scripts/ueditor/1.4.2/themes/default/css/ueditor.css" rel="stylesheet" />
    <link href="~/Content/Themes/Base/ztree/zTreeStyle.css" rel="stylesheet" />

    <!-–[if lt IE7]>
    <link href="~/Content/Themes/Base/css/font-awesome-ie7.min.css" rel="stylesheet" />
    <![endif]–->
    <!-–[if lt IE9]>
    <script src="~/Scripts/html5.js" type="text/javascript"></script>
    <![endif]–->
    <script src="~/Scripts/jquery-1.8.2.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Belvoly/beyondbit-2.0.0.min.js" type="text/javascript"></script>
    <script src="~/Scripts/Belvoly/Plug/jquery.validate.belvoly.js"></script>
    <script src="~/Scripts/jqueryui/1.11/jquery-ui.js"></script>
    <script src="~/Scripts/jqueryui/1.11/i18n/jquery.ui.datepicker-zh-CN.min.js"></script>
    <script src="~/Scripts/ueditor/1.4.2/ueditor.config.js"></script>
    <script src="~/Scripts/ueditor/1.4.2/ueditor.all.js"></script>
    <script src="~/Scripts/ztree/3.5/jquery.ztree.all-3.5.js"></script>
    <link href="~/Content/Themes/Meeting/css/uploadify.css" rel="stylesheet" />
    <script src="~/Scripts/uploadify/3.2/jquery.uploadify.min.js?ver=@DateTime.Now.Ticks"></script>
    <script src="~/Scripts/uploadify/3.2/jquery.uploadify-zh-CN.js"></script>
    <script src="~/Scripts/uploadify/UploadControl.js"></script>
    <script src="~/Scripts/ControlDatepicker.js"></script>
    @RenderSection("head", false)
</head>
<body>
    @RenderBody()

    <script src="~/Scripts/Belvoly/beyondbit.mvc-2.0.0.min.js" type="text/javascript"></script>
    @{
        var initForm = ViewBag.InitFormData as Dictionary<string, System.Collections.IDictionary>;
        if (initForm != null && initForm.Count > 0)
        {
            <script type="text/javascript">
                $(function () {
                    @foreach (var dict in initForm)
                        {
                        <text>
                    $("#@dict.Key").initMvcForm(@Html.Raw(Json.Encode(dict.Value)));
                    </text>
                        }
                })
            </script>
        }
    }
    @RenderSection("script", false)
</body>
</html>
